{
  "name": "factory-frontend",
  "version": "0.0.1",
  "description": "Web frontend for factory needs",
  "productName": "Factory Frontend",
  "author": "Arnaud Rocher <arnaud.roche3@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.json,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run\"",
    "test:component": "cross-env NODE_ENV=test cypress open-ct",
    "test:component:ci": "cross-env NODE_ENV=test cypress run-ct"
  },
  "dependencies": {
    "@pinia/testing": "0.0.11",
    "@quasar/extras": "1.13.5",
    "@vueuse/core": "8.2.6",
    "locale-matcher": "2.0.0",
    "pinia": "2.0.13",
    "quasar": "2.6.6",
    "vue": "3.2.31",
    "vue-i18n": "9.2.0-beta.35",
    "vue-router": "4.0.14",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "1.4.1",
    "@intlify/vite-plugin-vue-i18n": "4.0.0",
    "@quasar/app-vite": "1.0.0-beta.14",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "4.1.0",
    "@types/color": "^3.0.3",
    "@types/node": "16.11.27",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "autoprefixer": "10.4.4",
    "color": "^4.2.3",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-vue": "8.6.0",
    "miragejs": "0.1.43",
    "prettier": "2.6.2",
    "typescript": "4.6.3",
    "vue-router-mock": "0.1.7"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
