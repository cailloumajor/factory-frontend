{
  "name": "factory-frontend",
  "version": "0.4.0",
  "description": "Web frontend for factory needs",
  "productName": "Factory Frontend",
  "author": "Arnaud Rocher <arnaud.roche3@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.json,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:e2e": "cross-env start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "1.15.5",
    "@vueuse/core": "9.4.0",
    "centrifuge": "2.8.5",
    "locale-matcher": "2.0.0",
    "mande": "2.0.3",
    "pinia": "2.0.23",
    "quasar": "2.10.1",
    "rxjs": "7.5.7",
    "vue": "3.2.41",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@intlify/vite-plugin-vue-i18n": "6.0.3",
    "@pinia/testing": "0.0.14",
    "@quasar/app-vite": "1.1.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "https://gitpkg.now.sh/cailloumajor/quasar-testing/packages/e2e-cypress?dev",
    "@rushstack/eslint-patch": "1.2.0",
    "@types/node": "18.11.9",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "autoprefixer": "10.4.13",
    "colortranslator": "1.9.3",
    "cypress": "^10.11.0",
    "dotenv": "16.0.3",
    "eslint": "8.26.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-vue": "9.7.0",
    "events": "^3.3.0",
    "miragejs": "0.1.45",
    "prettier": "2.7.1",
    "prettier-plugin-jsdoc": "0.4.2",
    "typescript": "4.8.4",
    "vite-plugin-compression": "0.5.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": "^1.21.1"
  }
}
